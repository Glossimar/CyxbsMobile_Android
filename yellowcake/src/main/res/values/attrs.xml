<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Yellowcake">
        <attr name="textColor" format="color" />
        <attr name="scale" format="float" />
        <attr name="emptyCenter" format="boolean" />
        <attr name="border" format="boolean" />
        <attr name="divide" format="boolean" />
        <attr name="cornerDegree" format="float" />
        <attr name="innerColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ColorTextListView">
        <attr name="max" format="integer" />
        <attr name="marginTop" format="dimension" />
        <attr name="gravity" format="enum" >
            <enum name="center" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
    </declare-styleable>
</resources>